{
  "env": {
    "browser": true,
    "es2017": true,
    "jquery": true
  },
  "extends": [
    "airbnb",
    "plugin:fsd/all"
  ],
  "globals": {
    "$": "readonly",
    "jQuery": "readonly",
    "L": "readonly",
    "autoBind": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "plugins": ["fsd"],
  "rules": {
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "no-new": "off",
    "class-methods-use-this": "off"
  }
}
